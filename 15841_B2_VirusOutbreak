#include <iostream>
using namespace std;

long long fiboo[491]={0,};

long long fibo(int n){
    if(n==1||n==2) return 1;
    else{
        if(fiboo[n]==0){
            fiboo[n]=fibo(n-1)+fibo(n-2);
            return fiboo[n];
        }
        else return fiboo[n];
    }
}

int main(){
    int aa=0;
    int counter=0;

    while(1){
        cin>>aa;
        counter++;
        if(aa==-1) break;
        cout<<"Hour "<<counter<<": "<<fibo(aa)<<"cow(s) affected"<<endl;
    }

    return 0;
}